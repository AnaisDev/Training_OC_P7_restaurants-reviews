@import './../../../../../variables';

.rating-bar {
    color: black;
    font-size: 1em;
    display: flex;
    align-items: center;
}

.icon-full-circle, .icon-empty-circle, .icon-semi-circle {
    color: $darkGreen;
    padding: 1px;
    text-shadow:1px 1px 1px rgba(34, 88, 61, 1)
}

p {
    color: $green;
    font-size: 1.3em;
    font-weight: bold;
    margin: 0px 5px 0 0;
    padding: 5px 0px 5px 0;
}